net: "train_val.prototxt"

# In this case, we have test batch size 64 and 156 test iterations, covering the full 10,000 testing images.
test_iter: 156

test_interval: 1000

base_lr: 0.001
lr_policy: "step"
gamma: 0.1

stepsize: 100000

momentum: 0.9
weight_decay: 0.0005

display: 20

max_iter: 200000
snapshot: 10000
snapshot_prefix: "/media/gavin/e2bcddc2-a2ea-4321-839e-29b2db8373dc/siamese_cats_dogs_snapshots/siamese_cats_dogs"

solver_mode: GPU
